    "def myfunc(mystring):\n",
    "    output = ''\n",
    "    index = 0\n",
    "    for letter in mystring:\n",
    "        if index % 2 == 0:\n",
    "            output += letter.upper()\n",
    "        else:\n",
    "            output += letter.lower()\n",
    "        index+=1\n",
    "    return output"
   ]
